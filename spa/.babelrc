{
    "env": {
        "development": {
            "presets": [
                [
                    "env",
                    {
                        "targets": "current"
                    }
                ]
            ],
            "sourceMaps": "inline",
            "retainLines": true
        },
        "browser": {
            "presets": [
                [
                    "env",
                    {
                        "targets": {
                            "browsers": [
                                "last 2 versions"
                            ]
                        }
                    }
                ]
            ],
            "plugins": [
                [
                    "transform-react-jsx",
                    {
                        "pragma": "h"
                    }
                ],
                "transform-runtime",
                "transform-object-rest-spread",
                "transform-react-constant-elements"
            ],
            "retainLines": true
        }
    }
}