{
  "name": "spa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p server:dev client:dev",
    "client:dev": "cross-env NODE_ENV=development nodemon -w config --exec \"webpack-dev-server --config config/webpack.config.babel.js\"",
    "client:build": "webpack --config config/webpack.config.babel.js --display-optimization-bailout",
    "server:dev": "cross-env NODE_ENV=development nodemon -w server --exec \"babel-node server\"",
    "server:build": "babel server -s -D -d dist",
    "server:start": "cross-env NODE_ENV=production node dist",
    "build": "run-p client:build server:build",
    "start": "cross-env WEBPACK_ENV=production run-s build server:start"
  },
