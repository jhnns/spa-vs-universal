{
  "name": "universal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "app:client:dev": "cross-env WEBPACK_ENV=development WEBPACK_TARGET=browser nodemon -w config --exec \"webpack-dev-server --config config/webpack.config.babel.js\"",
    "app:server:dev": "cross-env WEBPACK_ENV=development WEBPACK_TARGET=node nodemon -w config --exec \"webpack --config config/webpack.config.babel.js --watch\"",
    "app:dev": "run-p app:client:dev app:server:dev",
    "server:dev": "cross-env NODE_ENV=development nodemon -w server -w dist/app --exec \"npm run server:build\"",
    "dev": "run-p server:dev app:dev",
    "app:client:cleanup": "rimraf dist/public/**",
    "app:server:cleanup": "rimraf dist/app/**",
    "server:cleanup": "rimraf dist/server/**",
    "cleanup": "run-p app:client:cleanup app:server:cleanup server:cleanup",
    "app:client:build": "cross-env WEBPACK_TARGET=browser webpack --config config/webpack.config.babel.js  --display-optimization-bailout",
    "app:server:build": "cross-env WEBPACK_TARGET=node webpack --config config/webpack.config.babel.js --display-optimization-bailout",
    "app:build": "run-p app:client:build app:server:build",
    "server:build": "babel server -s -D -d dist/server",
    "build": "run-p app:build server:build",
    "cleanup-build": "run-s cleanup build",
    "server:start": "node dist/server",
    "server:start-dev": "cross-env NODE_ENV=development nodemon -w dist/server --exec \"node dist/server\"",
    "start-dev": "run-p dev server:start-dev",
    "start": "cross-env WEBPACK_ENV=production run-s cleanup-build server:start"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "compression-webpack-plugin": "^0.4.0",
    "connect-gzip-static": "^2.1.1",
    "cookie": "^0.3.1",
    "copy-webpack-plugin": "^4.5.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "csurf": "^1.9.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "from-now": "^1.0.2",
    "glamor": "^3.0.0-3",
    "glamor-server": "^3.0.0-3",
    "helmet": "^3.12.0",
    "hex-to-rgba": "^0.2.0",
    "image-webpack-loader": "^3.6.0",
    "isomorphic-unfetch": "^2.0.0",
    "jsonwebtoken": "^7.4.3",
    "kewler": "^1.0.8",
    "mock-rest-middleware": "^2.3.1",
    "morgan": "^1.9.0",
    "nanohistory": "^1.0.0",
    "nanohref": "^3.0.1",
    "nanorouter": "^2.2.0",
    "node-fetch": "^1.7.3",
    "npm-run-all": "^4.1.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "preact": "^8.2.7",
    "preact-render-to-string": "^3.7.0",
    "readable-regex": "^1.0.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "serialize-javascript": "^1.4.0",
    "source-map-support": "^0.4.18",
    "stream-template": "0.0.4",
    "transform-loader": "^0.2.4",
    "unassertify": "^2.1.0",
    "unfetch": "^3.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-peerigon": "^11.3.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.1",
    "webpack-dev-server": "^2.11.2"
  }
}
